{
  "name": "react-ocl",
  "version": "5.0.0",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "files": [
    "lib",
    "lib-cjs",
    "base.d.ts",
    "base.mjs",
    "base.js",
    "core.d.ts",
    "core.mjs",
    "core.js",
    "full.d.ts",
    "full.mjs",
    "full.js",
    "minimal.d.ts",
    "minimal.mjs",
    "minimal.js",
    "types.d.ts"
  ],
  "main": "minimal.js",
  "module": "minimal.mjs",
  "types": "minimal.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/zakodium-oss/react-ocl.git"
  },
  "homepage": "https://github.com/zakodium-oss/react-ocl#readme",
  "bugs": "https://github.com/zakodium-oss/react-ocl/issues",
  "author": "MichaÃ«l Zasso <targos@protonmail.com>",
  "scripts": {
    "build-storybook": "build-storybook",
    "compile": "babel src --out-dir lib --ignore src/**/__tests__ --quiet --source-maps",
    "compile-cjs": "cross-env NODE_ENV=cjs babel src --out-dir lib-cjs --ignore src/**/__tests__ --quiet --source-maps",
    "dev": "start-storybook -p 6006",
    "eslint": "eslint src stories",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "npm run compile && npm run compile-cjs",
    "prettier": "prettier --check src",
    "prettier-write": "prettier --write src",
    "test": "npm run test-only && npm run eslint && npm run prettier",
    "test-only": "cross-env NODE_ENV=test jest"
  },
  "prettier": {
    "arrowParens": "always",
    "tabWidth": 2,
    "singleQuote": true,
    "semi": true,
    "trailingComma": "all"
  },
  "license": "MIT",
  "volta": {
    "node": "16.16.0"
  },
  "peerDependencies": {
    "openchemlib": ">=8",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "eslint-config-cheminfo-react": "^9.0.2",
    "jest": "^29.1.2",
    "openchemlib": "^8.0.1",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0"
  }
}
