    "jsdom": "^28.0.0",
    "openchemlib": "^9.19.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rimraf": "^6.1.2",
    "storybook": "^10.2.8",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "repository": {
