{
  "name": "react-ocl",
  "version": "8.0.1",
  "license": "MIT",
  "description": "React components integrating OpenChemLib",
  "keywords": [
    "react",
    "react-component",
    "ocl",
    "openchemlib"
  ],
  "author": "MichaÃ«l Zasso",
  "type": "module",
  "files": [
    "lib",
    "src"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build-storybook": "storybook build",
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib",
    "dev": "storybook dev -p 6006",
    "eslint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "prepack": "npm run tsc",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run test-only && npm run test-storybook && npm run check-types && npm run eslint && npm run prettier",
    "test-only": "vitest run --project=unit --coverage",
    "test-storybook": "vitest run --project=storybook",
    "tsc": "npm run clean && npm run tsc-build",
    "tsc-build": "tsc -p tsconfig.build.json"
  },
  "peerDependencies": {
    "openchemlib": ">=8",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@storybook/addon-vitest": "^9.1.2",
    "@storybook/react-vite": "^9.1.2",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@zakodium/tsconfig": "^1.0.2",
    "eslint": "^9.33.0",
    "eslint-config-cheminfo-react": "^17.0.1",
    "eslint-config-cheminfo-typescript": "^19.0.0",
    "eslint-plugin-storybook": "^9.1.2",
    "jsdom": "^26.1.0",
    "openchemlib": "^9.7.0",
    "playwright": "^1.55.0",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zakodium-oss/react-ocl.git"
  },
  "bugs": "https://github.com/zakodium-oss/react-ocl/issues",
  "homepage": "https://github.com/zakodium-oss/react-ocl#readme",
  "volta": {
    "node": "24.6.0"
  }
